<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
 <package name="struts2_login" extends="struts-default" namespace="/">
		<action name="loginresult" method="loginresult" class="com.bbs.action.userLR.UserLRAction">
			<result name="success">/success.jsp</result>
			<result name="login">/login.jsp</result>
		</action>
	</package>
	
	
    <package name="struts2_register" extends="struts-default" namespace="/">
		<action name="registerresult" method="registerresult" class="com.bbs.action.userLR.UserLRAction">
			<result name="success">/success.jsp</result>
			<result name="register">/register.jsp</result>
		</action>
	</package>
</struts>